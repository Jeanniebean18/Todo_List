<%= @user.errors.full_messages %>

<h1>Hi. Tell us about yourself.</h1> 

<%= form_tag users_path, :method => "post" do %>
  <p>
  Email: <input type="email" placeholder="your email" name="user[email]" 
  value="<%=params[:user][:email]%>" required>
  </p>
  <p>
  Password: <input type="password" pattern=".{5,10}" placeholder="password" 
  name="user[password]" value="" required title="5 to 10 characters">
  </p>
  <p>
  <input type="submit" value="submit">
  </p>
<%end%>